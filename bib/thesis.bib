  file = {C\:\\Users\\mbrxsmbc\\Zotero\\storage\\7H73EKWQ\\Hippisley-Cox et al. - 2017 - Development and validation of QRISK3 risk predicti.pdf;C\:\\Users\\mbrxsmbc\\Zotero\\storage\\82FFSSH4\\bmj.html},
  journal = {BMJ},
  language = {en},
