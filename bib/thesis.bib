@article{bleeker_external_2003,
  title = {External Validation Is Necessary in Prediction Research: A Clinical Example},
  shorttitle = {External Validation Is Necessary in Prediction Research},
  author = {Bleeker, S. E. and Moll, H. A. and Steyerberg, E. W. and Donders, A. R. T. and {Derksen-Lubsen}, G. and Grobbee, D. E. and Moons, K. G. M.},
  year = {2003},
  month = sep,
  volume = {56},
  pages = {826--832},
  issn = {0895-4356},
  doi = {10.1016/s0895-4356(03)00207-5},
  abstract = {BACKGROUND AND OBJECTIVES: Prediction models tend to perform better on data on which the model was constructed than on new data. This difference in performance is an indication of the optimism in the apparent performance in the derivation set. For internal model validation, bootstrapping methods are recommended to provide bias-corrected estimates of model performance. Results are often accepted without sufficient regard to the importance of external validation. This report illustrates the limitations of internal validation to determine generalizability of a diagnostic prediction model to future settings.
METHODS: A prediction model for the presence of serious bacterial infections in children with fever without source was derived and validated internally using bootstrap resampling techniques. Subsequently, the model was validated externally.
RESULTS: In the derivation set (n=376), nine predictors were identified. The apparent area under the receiver operating characteristic curve (95\% confidence interval) of the model was 0.83 (0.78-0.87) and 0.76 (0.67-0.85) after bootstrap correction. In the validation set (n=179) the performance was 0.57 (0.47-0.67).
CONCLUSION: For relatively small data sets, internal validation of prediction models by bootstrap techniques may not be sufficient and indicative for the model's performance in future patients. External validation is essential before implementing prediction models in clinical practice.},
  journal = {Journal of Clinical Epidemiology},
  keywords = {Bacterial Infections,Child; Preschool,Confidence Intervals,Fever of Unknown Origin,Humans,Infant,Likelihood Functions,Predictive Value of Tests},
  language = {eng},
  number = {9},
  pmid = {14505766}
}

